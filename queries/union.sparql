PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX foaf: <http://xmlns.com/foaf/0.1/>

SELECT ?page ?type ?name WHERE
{
  ?s foaf:page ?page .
  { ?s rdfs:label "Microsoft"@en }
  UNION
  { ?s rdfs:label "Apple"@en }
  UNION
  { ?s rdfs:label "Alphabet"@en }
}
